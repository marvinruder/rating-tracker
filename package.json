{
  "name": "rating-tracker",
  "version": "0.1.0",
  "title": "Rating Tracker",
  "author": {
    "name": "Marvin A. Ruder",
    "url": "https://mruder.dev"
  },
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:server": "conc --kill-others \"yarn workspace rating-tracker-backend dev:ts-node\" \"yarn workspace rating-tracker-frontend dev:vite\" \"yarn workspace rating-tracker-commons dev:build\" -n \",,ﯤ\" -p \"{name}\" -c #3178C6,#61DAFB,#3178C6 --timings",
    "dev:tools": "conc --kill-others \"yarn workspace rating-tracker-backend dev:tools\" -n \"\" -p \"{name}\" -c grey --timings",
    "test": "yarn workspaces foreach -pt run test",
    "build": "yarn workspaces foreach -pt run build",
    "lint": "yarn workspaces foreach -pt run lint"
  },
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  }
}
