{
  "name": "@rating-tracker/commons",
  "version": "6.0.0",
  "title": "Rating Tracker Common Types and Utilities",
  "author": {
    "name": "Marvin A. Ruder",
    "email": "ratingtracker@mruder.dev",
    "url": "https://mruder.dev"
  },
  "license": "MIT",
  "type": "module",
  "exports": "./src/index.ts",
