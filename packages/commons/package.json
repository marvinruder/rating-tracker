{
  "name": "@rating-tracker/commons",
  "version": "2.1.8",
  "title": "Rating Tracker Common Types and Utilities",
  "author": {
    "name": "Marvin A. Ruder",
    "url": "https://mruder.dev"
  },
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "dev:build": "yarn build --watch",
    "build": "esbuild --color=true src/index.ts --bundle --minify --platform=node --format=esm --legal-comments=none --outfile=dist/index.js",
    "test:watch": "vitest",
    "test": "vitest run --color",
    "lint": "eslint --cache --ext .ts src/",
    "lint:fix": "eslint --cache --ext .ts src/ --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@vitest/coverage-v8": "0.34.1",
    "esbuild": "0.19.0",
    "eslint": "8.47.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsdoc": "46.4.6",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitest": "0.34.1"
  }
}
