{
  "name": "@rating-tracker/frontend",
  "version": "2.3.0",
  "title": "Rating Tracker Frontend",
  "author": {
    "name": "Marvin A. Ruder",
    "url": "https://mruder.dev"
  },
  "license": "MIT",
  "scripts": {
    "dev:vite": "vite --host",
    "build": "vite build",
    "test:watch": "vitest",
    "test": "vitest run --color",
    "typecheck": "tsc --noEmit --tsBuildInfoFile $HOME/.cache/rating-tracker/frontend.tsbuildinfo",
    "lint": "eslint --color --cache --cache-location $HOME/.cache/rating-tracker/frontend.eslintcache --cache-strategy content --max-warnings 0 --ext .tsx,.ts src/",
    "lint:fix": "yarn lint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.12",
    "@mui/lab": "5.0.0-alpha.147",
    "@mui/material": "5.14.12",
    "@rating-tracker/commons": "workspace:*",
    "@rating-tracker/wasm": "workspace:*",
    "@simplewebauthn/browser": "8.3.1",
    "@types/react": "18.2.26",
    "@types/react-dom": "18.2.12",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/web-worker": "0.34.6",
    "apexcharts": "3.43.0",
    "axios": "1.5.1",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "nprogress": "0.2.0",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-apexcharts": "1.4.1",
    "react-dom": "18.2.0",
    "react-router": "6.16.0",
    "react-router-dom": "6.16.0",
    "react-switch-selector": "2.2.1",
    "react-tsparticles": "2.12.2",
    "tsparticles-engine": "2.12.0",
    "tsparticles-slim": "2.12.0",
    "vite": "4.4.11",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-top-level-await": "1.3.1",
    "vite-plugin-wasm": "3.2.2",
    "vite-tsconfig-paths": "4.2.1"
  },
  "devDependencies": {
    "@types/node": "20.8.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.51.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
