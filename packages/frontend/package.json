    "@rating-tracker/wasm": "workspace:*",
    "@simplewebauthn/browser": "8.3.4",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@vitejs/plugin-react": "4.1.1",
    "@vitest/web-worker": "0.34.6",
