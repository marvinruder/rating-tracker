{
  "name": "rating-tracker-backend",
  "version": "0.1.0",
  "title": "Rating Tracker",
  "author": {
    "name": "Marvin A. Ruder",
    "url": "https://mruder.dev"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "19.1.0"
  },
  "scripts": {
    "dev:ssh": "ssh -N rating-tracker-port-forwarding",
    "dev:tsc": "tsc --watch",
    "dev:node": "node --watch dist/src/server.js",
    "test:watch": "yarn node --experimental-vm-modules $(yarn bin jest) --watch --runInBand",
    "start": "node dist/src/server.js",
    "build": "tsc --project tsconfig.build.json",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --runInBand --coverage",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/",
    "lint:fix": "eslint --ext .jsx,.js,.tsx,.ts src/ --fix"
  },
  "dependencies": {
    "@simplewebauthn/server": "6.2.2",
    "@simplewebauthn/typescript-types": "6.3.0-alpha.1",
    "axios": "1.1.3",
    "chalk": "5.1.2",
    "cookie-parser": "1.4.6",
    "cron": "2.1.0",
    "date-fns": "2.29.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "express-openapi-validator": "5.0.0",
    "express-rate-limit": "6.7.0",
    "rating-tracker-commons": "workspace:*",
    "redis": "4.5.0",
    "redis-om": "0.3.6",
    "response-time": "2.3.2",
    "selenium-webdriver": "4.6.1",
    "swagger-ui-express": "4.6.0",
    "typescript": "4.9.3",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/selenium-webdriver": "4.1.9",
    "@types/supertest": "2.0.12",
    "@types/validator": "13.7.10",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "concurrently": "7.6.0",
    "eslint": "8.28.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1"
  }
}
