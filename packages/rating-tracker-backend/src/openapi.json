  "paths": {
    "/api/stock/details/{ticker}": {
      "get": {
        "summary": "Stock API",
        "description": "Get a list of all stocks known to the service with all available attributes",
        "parameters": [
          {
            "in": "path",
            "name": "ticker",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "example": "AAPL"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Stock"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Ticker not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/api/stock/list": {
      "get": {
        "summary": "Stock API",
        "description": "Get a list of all stocks tickers known to the service",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "example": "AAPL"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
