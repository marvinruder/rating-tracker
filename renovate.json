{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "timezone": "UTC",
  "schedule": ["after 7am"],
  "automerge": true,
  "automergeType": "branch",
  "automergeSchedule": ["after 7am"],
  "assignees": ["ruder.marvin@icloud.com"],
  "updateLockFiles": true,
  "postUpdateOptions": ["yarnDedupeHighest"],
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "matchPackageNames": ["jimp"],
      "automerge": false
    }
  ]
}
